{
    "name": "action-slack-message",
    "version": "0.0.1",
    "private": true,
    "description": "TypeScript template action",
    "main": "lib/main.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write **/*.ts",
        "format-check": "prettier --check **/*.ts",
        "lint": "eslint src/**/*.ts",
        "pack": "ncc build --source-map --license licenses.txt",
        "prebuild": "rimraf \"lib/*\"",
        "prepack": "rimraf \"dist/*\"",
        "test": "mocha \"tests/**/*.test.ts\"",
        "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/actions/typescript-action.git"
    },
    "keywords": [
        "actions",
        "node",
        "setup"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.2.6",
        "@actions/github": "^4.0.0",
        "@octokit/core": "^3.4.0",
        "@slack/web-api": "^6.1.0"
    },
    "devDependencies": {
        "@ts-tools/node": "^2.1.1",
        "@types/chai": "^4.2.16",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.14.37",
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "@vercel/ncc": "^0.28.0",
        "chai": "^4.3.4",
        "eslint": "^7.23.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-github": "^4.1.3",
        "eslint-plugin-no-only-tests": "^2.5.0",
        "js-yaml": "^4.0.0",
        "mocha": "^8.3.2",
        "prettier": "2.2.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.2.4"
    }
}
